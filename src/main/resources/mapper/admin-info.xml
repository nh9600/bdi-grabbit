<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.grabit.bdi.mapper.AdminInfoMapper">

	<select id="selectAdminInfo" parameterType="admin" resultType="admin"> 
		select * from admin_info
		where ai_id = #{aiId} and ai_pwd = #{aiPwd}
	</select>
	
</mapper>